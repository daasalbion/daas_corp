<!--Start Breadcrumb-->
<div class="row">
    <div id="breadcrumb" class="col-xs-12">
        <ol class="breadcrumb">
            <li><a href="#">Dashboard</a></li>
            <button id="cerrar_ventana_principal" style="float: right" class="btn btn-danger">Cerrar</button>
            <button id="abrir_ventana_principal" style="float: right" class="btn btn-danger">Jugar</button>
        </ol>
    </div>
</div>
<!--End Breadcrumb-->
<div class="row">
    <div class="col-xs-12">
        <div class="box">
            <div class="box-header">
                <div class="box-name">
                    <i class="fa fa-table"></i>
                    <span><strong>Tragamonedas</strong></span>
                </div>
                <div class="box-icons">
                    <a class="collapse-link">
                        <i class="fa fa-chevron-up"></i>
                    </a>
                    <a class="expand-link">
                        <i class="fa fa-expand"></i>
                    </a>
                    <a class="close-link">
                        <i class="fa fa-times"></i>
                    </a>
                </div>
                <div class="no-move"></div>
            </div>
            <div class="box-content no-padding">
                <table class="table table-striped table-bordered table-hover table-heading no-border-bottom">
                    <thead>
                        <tr>
                            <th class="col-sm-2">Preview</th>
                            <th class="col-sm-2">Sortear</th>
                            <th class="col-sm-3">Historial Ganadores</th>
                            <th class="col-sm-2">Cargar</th>
                            <th class="col-sm-2">Jugar</th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr>
                            <td><img class="juego_preview" src="/img/tvchat/interfaz/tvchat_tragamonedas_preview.png"></td>
                            <td>
                                <button id="getWinElementsTragamonedas" type="button" class="btn btn-primary">Sortear</button>
                                <div id="WinElementsTragamonedas">
                                    <p></p>
                                </div>
                            </td>
                            <td>
                                <div id="historial_tragamonedas">
                                </div>
                            </td>
                            <td>
                                <button id="cargar_tragamonedas" class="btn btn-primary">Cargar</button>
                                <button id="cerrar_tragamonedas" class="btn btn-primary">Quitar</button>
                            </td>
                            <td>
                                <button id="jugarTragamonedas" class="btn btn-primary">Jugar</button>
                            </td>
                        </tr>
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>
